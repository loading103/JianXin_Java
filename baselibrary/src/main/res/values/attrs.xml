<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IMAVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="TabButton">
        <attr name="tbn_selected_icon" format="reference"/>
        <attr name="tbn_unselected_icon" format="reference"/>
        <attr name="tbn_tip" format="string"/>
        <attr name="tbn_icon_size" format="dimension"/>
        <attr name="tbn_text_size" format="dimension"/>
        <attr name="tbn_text_color" format="color"/>
        <attr name="tbn_checked" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SettingsOptionView">
        <attr name="left_img" format="reference"/>
        <attr name="text_content" format="reference|string"/>
        <attr name="text_right" format="reference|string"/>
        <attr name="is_Check" format="reference|boolean"/>
        <attr name="is_show_line" format="reference|boolean"/>
        <attr name="is_show_right_arr" format="reference|boolean"/>
    </declare-styleable>
</resources>